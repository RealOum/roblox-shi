            ToggleCircle.Image = "rbxassetid://3570695787"
            ToggleCircle.ScaleType = Enum.ScaleType.Slice
            ToggleCircle.SliceCenter = Rect.new(100, 100, 100, 100)
            ToggleCircle.SliceScale = 0.120

            ToggleGlow.Name = "ToggleGlow"
            ToggleGlow.Parent = ToggleF
            ToggleGlow.BackgroundTransparency = 1.000
            ToggleGlow.Position = UDim2.new(0.5, -22, 0.5, -7)
            ToggleGlow.Size = UDim2.new(0, 45, 0, 30)
